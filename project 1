    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Assuming cleaned_budget_gross is preloaded with your data\n",
    "\n",
    "# Apply the mapping to ensure consistent studio names\n",
    "cleaned_budget_gross['Mapped_Studio'] = cleaned_budget_gross['studio'].map(studio_mapping).fillna(cleaned_budget_gross['studio'])\n",
    "\n",
    "# Filter the DataFrame to include only the selected studios\n",
    "filtered_budget_gross = cleaned_budget_gross[cleaned_budget_gross['Mapped_Studio'].isin(selected_studios)]\n",
    "\n",
    "# Calculate the average and total production budget for the filtered studios\n",
    "studio_budgets = filtered_budget_gross.groupby('Mapped_Studio').agg(\n",
    "    Average_Budget=pd.NamedAgg(column='production_budget', aggfunc='mean'),\n",
    "    Total_Budget=pd.NamedAgg(column='production_budget', aggfunc='sum')\n",
    ").reset_index()\n",
    "\n",
    "# Sort studios alphabetically by Mapped_Studio for consistent ordering\n",
    "studio_budgets_sorted = studio_budgets.sort_values(by='Mapped_Studio')\n",
    "\n",
    "# Set up the figure and axes for the subplots\n",
    "fig, axs = plt.subplots(1, 2, figsize=(18, 8))  # 1 row, 2 columns\n",
    "\n",
    "# Average Production Budget\n",
    "axs[0].bar(studio_budgets_sorted['Mapped_Studio'], studio_budgets_sorted['Average_Budget'], color=colors)\n",
    "axs[0].set_title('Average Production Budget')\n",
    "axs[0].set_ylabel('Average Budget ($)')\n",
    "axs[0].tick_params(axis='x', rotation=45)\n",
    "\n",
    "# Adding average budget annotations\n",
    "for bar in axs[0].containers[0]:\n",
    "    height = bar.get_height()\n",
    "    axs[0].text(bar.get_x() + bar.get_width() / 2., height, f'${height/1e6:.2f}M', ha='center', va='bottom')\n",
    "\n",
    "# Total Production Budget\n",
    "axs[1].bar(studio_budgets_sorted['Mapped_Studio'], studio_budgets_sorted['Total_Budget'], color=colors)\n",
