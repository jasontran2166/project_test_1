   ],
   "source": [
    "# Aggregate production budgets by studio to calculate the average budget\n",
    "studio_budgets_avg = cleaned_budget_gross.groupby('studio').agg(\n",
    "    Average_Budget=pd.NamedAgg(column='production_budget', aggfunc='mean')\n",
    ").reset_index()\n",
    "\n",
    "# Apply name mapping to unabbreviate studio names\n",
    "studio_budgets_avg['studio_full_name'] = studio_budgets_avg['studio'].map(studio_name_mapping).fillna(studio_budgets_avg['studio'])\n",
    "\n",
    "# Sort by Average Budget for top studios\n",
    "top_studios_avg = studio_budgets_avg.sort_values(by='Average_Budget', ascending=False).head(10)\n",
    "\n",
    "# Visualization of Average Production Budget\n",
    "plt.figure(figsize=(14, 8))\n",
    "bars = plt.bar(top_studios_avg['studio_full_name'], top_studios_avg['Average_Budget'], color=colors)\n",
    "\n",
    "# Adding average budget annotations on each bar\n",
    "for bar in bars:\n",
    "    height = bar.get_height()\n",
    "    plt.text(bar.get_x() + bar.get_width() / 2., height, f'${height/1e6:.2f}M', ha='center', va='bottom')\n",
    "\n",
    "plt.title('Top 10 Studios by Average Production Budget')\n",
    "plt.xlabel('Studio')\n",
    "plt.ylabel('Average Production Budget ($)')\n",
    "plt.xticks(rotation=45, ha='right')\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
